{
    "extends": "airbnb",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "import/order": 0,
        "no-trailing-spaces": 1,
        "no-param-reassign": 0,
        "arrow-body-style": 0,
        "no-undef": 0,
        "class-methods-use-this": 0,
        "no-useless-escape": 0,
        "jsx-a11y/href-no-hash": "off",
        "jsx-a11y/anchor-is-valid": ["warn", {
            "aspects": ["invalidHref"]
        }],
        "indent": ["error", 4],
        "camelcase": 0
    }
}
